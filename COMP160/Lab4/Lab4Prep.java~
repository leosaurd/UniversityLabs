





public class {
  public static void main(String[] args){
    int iResult, num1 = 25, num2 = 40, num3 = 17, num4 = 5; 
    double fResult, val1 = 17.0;
    
    iResult = num1 / num4; 
    System.out.println(iResult);
    fResult = num1 / num4; 
    System.out.println(fResult);
    iResult = num3 / num4; 
    System.out.println(iResult);
    fResult = num3 / num4; 
    System.out.println(fResult);
    fResult = val1 / num4;
    System.out.println(fResult);
    iResult = num1 / num2;
    System.out.println(iResult);
    iResult = num3 % num4;
    System.out.println(iResult);
    iResult = num2 % num3;
    System.out.println(iResult);
    iResult = num3 % num2;
    System.out.println(iResult);
    iResult = num2 % num4;
    System.out.println(iResult);
  }
}